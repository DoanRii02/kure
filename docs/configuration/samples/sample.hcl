// Paths must be absolute.
// In case any of these values is omitted, Kure will use the default one.
// See ../configuration.md for further information.

"clipboard" = {
  "timeout" = "5s"
}

"database" = {
  "path" = "/home/user/kure.db"
}

"editor" = "vim"

"keyfile" = {
  "path" = "/home/user/sample.key"
}

"session" = {
  "prefix" = "kure:~$"

  "timeout" = "10m"
}